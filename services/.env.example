# Environment configuration for microservices
# Copy this file to .env and set secure values for production

# Security
JWT_SECRET=changeThisToSecureRandomValue
SECRET_KEY=changeThisToSecureRandomValue
SERVICE_SECRET_KEY=changeThisToSecureRandomValue

# Database
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_DB=auth_db

# API Gateway
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX=100
CORS_ORIGIN=http://localhost:3000,http://localhost:5173
